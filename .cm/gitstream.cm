# -*- mode: yaml -*-

manifest:
  version: 1.0

automations:
  percent_new_code:
    if:
      - {{ files | extensions | match(term='java') }}
      - {{ source.diff.files | map(attr='original_file') | some) }}
    run: 
      - action: add-comment@v1
        args:
          comment: |
            This PR is  new code.

